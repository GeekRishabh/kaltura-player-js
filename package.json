{
  "name": "kaltura-player-js",
  "version": "0.41.2",
  "main": "dist/kaltura-player.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "npm run clean",
    "build:ovp": "NODE_ENV=production webpack --config webpack.config.ovp.js --mode production",
    "dev:ovp": "webpack-dev-server --config webpack.config.ovp.js",
    "watch:ovp": "webpack --progress --colors --watch --config webpack.config.ovp.js",
    "build:ott": "NODE_ENV=production webpack --config webpack.config.ott.js",
    "dev:ott": "webpack-dev-server --config webpack.config.ott.js",
    "watch:ott": "webpack --progress --colors --watch --config webpack.config.ott.js",
    "test": "NODE_ENV=test karma start --color",
    "release": "standard-version",
    "publish": "git push --follow-tags --no-verify origin master",
    "eslint": "eslint . --color",
    "flow": "flow check",
    "commit:dist": "git add --force --all dist && (git commit -m 'chore: update dist' || exit 0)",
    "playkit-dev-ovp:start": "playkit-dev start ovp",
    "playkit-dev-ott:start": "playkit-dev start ott",
    "playkit-dev:stop": "playkit-dev stop",
    "playkit-rel": "playkit-rel start",
    "precommit": "lint-staged",
    "docs:generate": "documentation build flow-typed/** src/** -f md -o docs/api.md",
    "docs:serve": "documentation serve flow-typed/** src/** --watch"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "envManager": {
    "devMode": {},
    "releaseMode": []
  },
  "standard-version": {
    "scripts": {
      "postbump": "yarn run build:ovp && yarn run build:ott && npm run commit:dist"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.23.0",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "documentation": "^8.1.2",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-mocha-no-only": "^0.0.6",
    "flow-bin": "0.71.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "lint-staged": "^7.2.0",
    "mocha": "^5.1.1",
    "mocha-cli": "^1.0.1",
    "prettier": "^1.13.7",
    "pre-push": "^0.1.1",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "source-map-loader": "^0.2.3",
    "standard-version": "^4.3.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "3.1.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "hls.js": "^0.8.9",
    "js-logger": "^1.3.0",
    "playkit-js": "https://github.com/kaltura/playkit-js.git#v0.27.1",
    "playkit-js-providers": "https://github.com/kaltura/playkit-js-providers.git#v2.3.0",
    "playkit-js-ui": "https://github.com/kaltura/playkit-js-ui.git#v0.23.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaltura/kaltura-player-js"
  },
  "keywords": [
    "kaltura",
    "player",
    "html5 player"
  ],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/kaltura/kaltura-player-js/issues"
  },
  "homepage": "https://github.com/kaltura/kaltura-player-js"
}
